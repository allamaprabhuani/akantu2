permissive_parser = true

model solid_mechanics_model [
material elastic [
	 name = steel
	 rho = 7800   # density
	 E   = 2.1e11 # young's modulus
	 nu  = 0.3    # poisson's ratio
   Plane_Stress = true # plane stress
   finite_deformation = false
]
dof_manager_type = petsc
time_step_solver dynamic [
name = tss
sparse_solver_type = petsc
]]