material elastic [
         name = bot_body
         rho = 7800   # density
         E   = 1.0 # young's modulu
         nu  = 0.0    # poisson's ratio
]

material elastic [
         name = top_body
         rho = 7800   # density
         E   = 1.0 # young's modulu
         nu  = 0.0   # poisson's ratio
]

contact_detector [
  type = implicit		 			
  slave = contact_top
  master = contact_bottom
  two_pass_algorithm = false
]

contact_resolution penalty [
  name = contact_top
  slave = contact_top
  master = contact_bottom
  mu = 0.0
  epsilon = 0.1
  epsilon_t = 100
  two_pass_algorithm = false
] 