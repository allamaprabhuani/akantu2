material elastic [
         name = bot_body
         rho = 7800   # density
         E   = 1e5 # young's modulu
         nu  = 0.    # poisson's ratio
]

material elastic [
         name = top_body
         rho = 7800   # density
         E   = 1 # young's modulu
         nu  = 0.    # poisson's ratio
]

contact_detector [
  type = implicit		 			
  master = contact_bottom
  slave = contact_top
]

contact_resolution penalty [
  name = contact_top
  mu = 0.0
  epsilon = 1e6
  epsilon_t = 100
] 